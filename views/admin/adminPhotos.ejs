    <div class="container admin-user-table">

        <table class="table table-striped table-hover table-responsive">
            <thead>
                <tr>
                    <th scope="col">Photo ID</th>
                    <th scope="col">Thumbnail</th>
                    <th scope="col">Aperture Value</th>
                    <th scope="col">Exposure Time</th>
                    <th scope="col">Exposure Value</th>
                    <th scope="col">Base Value</th>
                </tr>
            </thead>
            <tbody class="">
                <% for (let data of datas) { %>

                    <tr class="position-relative">
                        <td scope="row">
                            <%= data.id %>
                        </td>
                        <td>
                            <img src="<%= data.imageUrl %>" alt="" class="w-100px">
                        </td>
                        <td>
                            <%= data.av %>
                        </td>
                        <td>
                            <%= data.tv %>
                        </td>
                        <td>
                            <%= data.ev %>
                        </td>
                        <td><%= data.sp %></td>
                    </tr>

                    <% } %>
            </tbody>
        </table>
    </div>
    <div class="d-flex justify-content-between my-3 container">
        <% if (prevCursor) { %>
            <a class="btn btn-secondary" href="?prev=<%= prevCursor %>">Previous</a>
        <% } %>
        <% if (nextCursor) { %>
            <a class="btn btn-secondary ms-auto" href="?cursor=<%= nextCursor %>&prev=<%= firstKey %>">Next</a>
        <% } %>
    </div>

    <button id="downloadButton" class="btn btn-primary mt-3">Download CSV</button>
    <script src="/downloadCSV.js"></script>

    <%- include('../partials/footer'); -%>
